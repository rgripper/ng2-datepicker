{
  "name": "ng2-datepicker",
  "version": "1.4.8",
  "description": "Angular2 Datepicker",
  "main": "ng2-datepicker.js",
  "typings": "ng2-datepicker.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jkuri/ng2-datepicker.git"
  },
  "scripts": {
