    "@angular/core": "^2.4.3",
    "@angular/forms": "^2.4.3",
    "@angular/platform-browser": "^2.4.3",
    "@types/node": "^6.0.42",
    "codelyzer": "^2.0.0-beta.4",
    "node-sass": "^4.3.0",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.3",
    "tslint": "^4.3.1",
    "typescript": "2.1.5",
    "zone.js": "0.7.5",

    "moment": "^2.17.1"
